    "def leap_year(year):\n",
    "    if year % 4 and year % 100!=0 or year % 400 == 0:\n",
    "        print('It is a leap year!!')\n",
    "    else:\n",
    "        print('Not a leap year!!')\n",
    "n = int(input(\"Enter the year to check: \"))\n",
    "leap_year(n)\n",
    "print(' Thank You ')"
